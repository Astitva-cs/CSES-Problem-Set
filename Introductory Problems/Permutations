#include <bits/stdc++.h>
using namespace std;
 
 
 
int main() {
	
	
	int n;cin>>n;
	
	
	if(n==1)
	{
	    cout<<1<<endl;
	    return 0;
	}
	if(n==2 || n==3)
	{
	    cout<<"NO SOLUTION"<<endl;
	    
	}
	else
	{
	    int ev=2;
	    while(ev<=n)
	    {
	        cout<<ev<<" ";
	        ev+=2;
	    }
	    int od=1;
	    while(od<=n)
	    {
	        cout<<od<<" ";
	        od+=2;
	        
	    }
	    
	    
	    cout<<endl;
	    
	}
 
	
	return 0;
}
