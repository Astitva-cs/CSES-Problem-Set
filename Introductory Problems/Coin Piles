#include <iostream>
using namespace std;
 
int main() {
	
	int t;
	cin>>t;
	while(t--)
	{
	    
	    long long int a,b;
	    cin>>a>>b;
	    
		int x,y;
		if(a<b)
		{
			x=a;
			y=b;
 
		}
		else
		{
			x=b;
			y=a;
		}
	    if((2*x-y)>=0 &&(2*x-y)%3==0)
		{
			cout<<"YES"<<endl;
		}
		else{
			cout<<"NO"<<endl;
		}
	}
	return 0;
}
